{
  "uuid": "fdm-smart-media-optimizer",
  "name": "FDM Smart Media Optimizer",
  "author": "Hasan Shahid",
  "description": "IDM-like smart extraction, optimization and media selection for FDM.",
  "version": "1.0.1",
  "icon": "icon.png",
  "mediaParser": true,
  "mediaListParser": true,
  "scripts": [
    "media_parser.js"
  ],
  "permissions": ["launchPython"],
  "dependencies": {
    "Python": { "minVersion": "3.8" }
  },
  "minApiVersion": 8,
  "minFeaturesLevel": 2,
  "targetApiVersion": 9,
  "allowedPaths": [
    "%LOCALAPPDATA%\\Google\\Chrome\\User Data",
    "%APPDATA%\\Mozilla\\Firefox\\Profiles",
    "~/.config/google-chrome",
    "~/.mozilla/firefox"
  ]
}
